{
  "name": "git-env",
  "displayName": "git-env",
  "description": "It's manage your github account.",
  "version": "0.0.1",
  "publisher": "Akshay-Dhola",
  "engines": {
    "vscode": "^1.92.0"
  },
  "icon": "gitenv.jpg",
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.start",
        "title": "gitenv Repo"
      },
      {
        "command": "extension.keepme",
        "title": "gitenv Track"
      },
      {
        "command": "extension.goon",
        "title": "gitenv Deploy"
      },
      {
        "command": "extension.update",
        "title": "gitenv Update"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "@types/vscode": "^1.92.0",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0",
    "eslint": "^8.57.0",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "axios": "^1.7.7"
  }
}
